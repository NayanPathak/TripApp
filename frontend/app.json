{
  "expo": {
    "name": "Trip Itinerary",
    "slug": "trip-itinerary-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
